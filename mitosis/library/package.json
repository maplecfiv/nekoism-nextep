{
  "name": "@mitosis/library",
  "private": true,
  "scripts": {
    "start": "watch \"npm run compile-tailwind && npm run build\" ./src",
    "compile-tailwind": "npx tailwindcss -i ./src/tailwindcss.input.css -o ./src/tailwindcss.css && cp ./src/tailwindcss.css  ../test-apps/react/",
    "build": "cp ../../node_modules/daisyui/dist/full.css ../test-apps/react/ && mitosis build --c mitosis.config.cjs",
    "lint": "eslint"
  },
  "type": "module",
  "exports": {
    "./*": "./output/*/src/index.js"
  },
  "dependencies": {
    "@builder.io/eslint-plugin-mitosis": "^0.0.15",
    "@builder.io/mitosis": "latest",
    "@builder.io/mitosis-cli": "latest",
    "@nextep/core": "file:../../core",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.51.0",
    "postcss": "^8.4.49",
    "sortablejs": "^1.15.3",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "daisyui": "^4.12.14",
    "tailwindcss": "^3.4.14",
    "watch": "^1.0.2"
  }
}
