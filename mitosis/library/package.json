{
  "name": "@mitosis/library",
  "private": true,
  "scripts": {
    "start": "watch \"npm run build\" ./src",
    "build": "npx tailwindcss -i ./src/input.css -o ./src/output.css && cp ./src/output.css ../test-apps/react/ && mitosis build --c mitosis.config.cjs",
    "lint": "eslint"
  },
  "type": "module",
  "exports": {
    "./*": "./output/*/src/index.js"
  },
  "dependencies": {
    "@builder.io/eslint-plugin-mitosis": "^0.0.15",
    "@builder.io/mitosis": "latest",
    "@builder.io/mitosis-cli": "latest",
    "@nextep/core": "file:../../core",
    "eslint": "^8.51.0"
  },
  "devDependencies": {
    "tailwindcss": "^3.4.14",
    "watch": "^1.0.2"
  }
}
